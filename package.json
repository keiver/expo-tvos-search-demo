{
  "name": "expo-tvos-search-demo",
  "version": "1.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "EXPO_TV=1 npx expo prebuild --clean",
    "tvos": "npx expo run:ios",
    "clear": "rm -rf .expo .metro-cache node_modules package-lock.json && npm i && yes | EXPO_TV=1 npx expo prebuild --clean && npm start"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-tvos/config-tv": "latest",
    "expo": "~54.0.31",
    "expo-linear-gradient": "~15.0.8",
    "expo-router": "~6.0.14",
    "expo-status-bar": "~3.0.9",
    "expo-tvos-search": "^1.2.3",
    "react": "19.1.0",
    "react-native": "npm:react-native-tvos@0.81.4-0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.10",
    "typescript": "~5.9.2"
  },
  "private": false
}